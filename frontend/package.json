{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:optimized": "node scripts/build-with-optimization.js",
    "start": "next start",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "vitest --run src/test/accessibility",
    "test:accessibility:watch": "vitest src/test/accessibility",
    "test:accessibility:ci": "vitest --run src/test/accessibility --reporter=json --outputFile=accessibility-results.json",
    "test:accessibility:comprehensive": "tsx src/test/accessibility/automated-accessibility-check.ts",
    "test:accessibility:quick": "tsx src/test/accessibility/accessibility-test-runner.ts --quick",
    "test:visual": "node scripts/run-visual-tests.js all",
    "test:visual:desktop": "node scripts/run-visual-tests.js desktop",
    "test:visual:mobile": "node scripts/run-visual-tests.js mobile",
    "test:visual:cross-browser": "node scripts/run-visual-tests.js crossBrowser",
    "test:visual:components": "node scripts/run-visual-tests.js components",
    "test:visual:accessibility": "node scripts/run-visual-tests.js accessibility",
    "test:visual:update": "node scripts/run-visual-tests.js all --update",
    "test:visual:clean": "node scripts/run-visual-tests.js all --clean",
    "test:visual:report": "playwright show-report playwright-report-visual",
    "docs:colors": "node scripts/generate-color-docs.js",
    "docs:colors:markdown": "node scripts/generate-color-docs.js --format=markdown",
    "docs:colors:json": "node scripts/generate-color-docs.js --format=json",
    "docs:colors:css": "node scripts/generate-color-docs.js --format=css",
    "validate:css": "node scripts/validate-css.js",
    "msw:init": "msw init public/ --save"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.1",
    "msw": "^2.11.3",
    "tailwindcss": "^4.1.14",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}